{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid"
    },
    "price": {
      "type": "integer"
    },
    "quantity": {
      "type": "integer"
    },
    "Catogary": {
      "type": "enumeration",
      "enum": [
        "Tshirt",
        "mug",
        "glass",
        "trouser"
      ]
    },
    "colors": {
      "type": "enumeration",
      "enum": [
        "red",
        "green",
        "yellow"
      ]
    },
    "pic": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text"
    }
  }
}
